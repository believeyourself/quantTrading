{
  "funding_rate_threshold": 0.001,
  "scan_interval_seconds": 1800,
  "cache_update_interval_seconds": 21600,
  "max_contracts_in_pool": 50,
  "min_volume_24h": 1000000,
  "telegram_notifications": {
    "enabled": true,
    "pool_changes": true,
    "pool_status": true,
    "error_alerts": true
  },
  "cache_settings": {
    "h1_contracts_cache_duration": 3600,
    "pool_cache_duration": 7200
  },
  "rate_limiting": {
    "requests_per_second": 10,
    "batch_size": 20,
    "delay_between_batches": 1.0
  },
  "logging": {
    "level": "INFO",
    "file_enabled": true,
    "console_enabled": true
  }
} 